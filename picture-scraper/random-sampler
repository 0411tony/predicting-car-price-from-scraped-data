from shutil import copyfile, copy
import os
os.chdir('f:/cars')
from random import choice
from pathlib import Path

newfolder = 'carsample'

if not os.path.isdir(os.path.join(Path(os.getcwd()).parent, newfolder)):
    os.mkdir(os.path.join(Path(os.getcwd()).parent, newfolder))

for i in range(5_000):
    copy(os.path.join(Path(os.getcwd()), choice(os.listdir())),
             os.path.join(Path(os.getcwd()).parent, newfolder))
             
